function twoNumberSum(arr, tar) {

    for(let i = 0; i < arr.length -1; i++) {
        let firstNum = arr[i];
        
        for(let j = 0; j < arr.length; j++) {
            let secondNum = arr[j];

            if (firstNum + secondNum == tar) {
                return [firstNum,secondNum]
            }
        }
    }
    return [];
}

module.exports = twoNumberSum;